import request from "@/utils/request";

//数据加工 -- 图像质检/质检和处理
//图像质检-查询任务列表(案卷)
export function queryTxzjTaskList(params) {
  return request({
    url: '/sjjg/txzj/queryTxzjTaskList',
    method: 'get',
    params: params
  })
}


