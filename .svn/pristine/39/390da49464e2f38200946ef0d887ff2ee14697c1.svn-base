import request from "@/utils/request";

//项目批次管理 --批次管理
//查询批次列表-已完成
export function queryCompletedBatchList(params) {
  return request({
    url: '/pcgl/queryCompletedBatchList',
    method: 'get',
    params: params
  })
}

//查询任务列表
export function queryCompletedTaskList(params) {
  return request({
    url: '/pcgl/queryCompletedTaskList',
    method: 'get',
    params: params
  })
}
